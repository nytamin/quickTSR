{
  "name": "quick-tsr",
  "version": "0.0.0",
  "description": "Read timeline from file, use TSR, play stuff",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/module/index.js",
  "browser": "dist/browser/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nytamin/quickTSR"
  },
  "author": {
    "name": "Johan Nyman",
    "email": "johan@superfly.tv",
    "url": "http://superfly.tv"
  },
  "contributors": [],
  "scripts": {
    "prepare": "husky install",
    "build": "rimraf dist && yarn build:main",
    "buildstart": "yarn build && yarn start",
    "buildinspect": "yarn build && yarn inspect",
    "build:main": "tsc -p tsconfig.build.json",
    "lint:raw": "eslint --ext .ts --ext .js --ext .tsx --ext .jsx --ignore-pattern dist",
    "lint": "yarn lint:raw .",
    "lint-fix": "yarn lint --fix",
    "inspect": "node --inspect dist/index.js",
    "start": "ts-node src/index.ts",
    "unlinkall": "yarn unlink timeline-state-resolver && yarn --check-files",
    "validate:dependencies": "yarn audit && yarn license-validate",
    "license-validate": "yarn sofie-licensecheck"
  },
  "prettier": "@sofie-automation/code-standard-preset/.prettierrc.json",
  "lint-staged": {
    "*.{css,json,md,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "yarn lint:raw --fix"
    ]
  },
  "engines": {
    "node": ">=12.20"
  },
  "devDependencies": {
    "@sofie-automation/code-standard-preset": "^1.1.0",
    "@types/node": "^16",
    "@types/underscore": "^1.9.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.4.1",
    "typescript": "~4.5"
  },
  "keywords": [
    "broadcast",
    "socket",
    "typescript",
    "javascript",
    "open",
    "source",
    "automation",
    "ncs",
    "nrcs",
    "rundown",
    "production"
  ],
  "dependencies": {
    "chokidar": "^3.2.1",
    "fast-clone": "^1.5.13",
    "timeline-state-resolver": "^6.4.0-release39.1",
    "tslib": "^2.3.1",
    "underscore": "^1.9.1"
  }
}
